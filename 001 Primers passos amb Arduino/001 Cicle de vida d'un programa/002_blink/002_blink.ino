//*********************************** INCLUDE *************************************

//********************************** VARIABLES ************************************

//*********************************** SETUP ***************************************
void setup()
{
  pinMode(13,OUTPUT);
}

//************************************ LOOP ***************************************
void loop()
{
  digitalWrite(13,HIGH);
  delay(200);
  digitalWrite(13,LOW);
  delay(200);
}

